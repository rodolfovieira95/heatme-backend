                    "country": {
                      "type": "string",
                      "example": "Brasil",
                      "description": "País do usuário",
                      "nullable": true
                    },
                    "city": {
                      "type": "string",
                      "example": "São Paulo",
                      "description": "Cidade do usuário",
                      "nullable": true
                    },
                    "bio": {
                      "type": "string",
                      "example": "Olá, sou um desenvolvedor apaixonado por tecnologia!",
                      "description": "Biografia do usuário",
                      "nullable": true
