                        "light brown",
                        "dark brown",
                        "black",
                        "red",
                        "auburn",
                        "gray",
                        "white",
                        "bald",
                        "dyed",
                        "other"
                      ],
                      "example": "blonde",
                      "description": "Cor do cabelo",
                      "nullable": true
                    },
                    "eyeColor": {
                      "type": "string",
                      "enum": [
                        "honey",
                        "light brown",
                        "dark brown",
                        "light blue",
                        "dark blue",
                        "light green",
                        "dark green",
                        "black",
                        "gray",
                        "hazel",
                        "amber",
                        "heterochromia",
                        "other"
                      ],
                      "example": "light brown",
                      "description": "Cor dos olhos",
                      "nullable": true
                    },
                    "preferredPosition": {
                      "type": "string",
                      "enum": [
                        "top",
                        "bottom",
                        "versatile",
                        "versatile + top",
                        "versatile + bottom",
                        "no penetration"
                      ],
                      "example": "versatile",
                      "description": "Posição sexual preferida",
                      "nullable": true
                    },
                    "tribe": {
                      "type": "array",
                      "items": {
                        "type": "string",
                        "enum": [
                          "twink",
                          "bear",
                          "chubby",
                          "chaser",
                          "daddy",
                          "jock",
                          "drag",
                          "otter",
                          "geek",
                          "leather",
                          "cub",
                          "chub",
                          "twunk",
                          "wolf",
                          "butch",
                          "femme",
                          "androgynous",
                          "trans",
                          "queer",
                          "non binary",
                          "genderfluid",
                          "other"
                        ]
                      },
                      "example": [
                        "cub",
                        "chubby"
                      ],
                      "description": "Tribos associadas ao usuário",
                      "nullable": true
                    },
                    "relationshipStatus": {
                      "type": "string",
                      "enum": [
                        "single",
                        "married",
                        "open_relationship",
                        "widowed",
                        "divorced",
                        "separated",
                        "complicated",
                        "polyamorous"
                      ],
                      "example": "single",
                      "description": "Status de relacionamento",
                      "nullable": true
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Perfil não encontrado"
          }
        },
        "security": [
          {
            "bearer": []
          }
        ],
        "summary": "Obter perfil do usuário atual",
        "tags": [
          "Perfis"
        ]
