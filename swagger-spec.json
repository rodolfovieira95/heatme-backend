                        "type_1",
                        "type_2",
                        "type_3",
                        "type_4",
                        "type_5",
                        "type_6",
                        "mixed",
                        "other"
                      ],
                      "example": "type_1",
                      "description": "Cor da pele",
                      "nullable": true
